<section class="wrapper style2 container special-alt">
  <div class="row half">
    <div class="8u">
      <div class="challenge-list-item">
        <a href="/challenges/<%=@challenge.id%>">
        <div class="challenge-list-item">
          <p><%=@challenge.name%></p>
          <date><%=@challenge.date.to_s%></date>
        </div>
        </a>
      </div>
    </div>
  </div>
</section>
<div class="container">
  <div class="main">
    <div class="gamma-container gamma-loading" id="gamma-container">

      <ul class="gamma-gallery">
        <% @challenge.images.each do |image| %>
        <li>
          <div data-alt="img03" data-description="<h3><%=image.title%></h3>" data-max-width="1800" data-max-height="1350">
            
            <div data-src="<%= image.url %>"></div>
            <noscript>
              <img src="images/xsmall/3.jpg" alt="img03"/>
            </noscript>
          </div>
        </li>
        <% end %>
      </ul>

      <div class="gamma-overlay"></div>

      <div id="loadmore" class="loadmore">Example for loading more items...</div>

    </div>

  </div><!--/main-->
</div>