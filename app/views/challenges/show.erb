<header class="special container">
  <span class="icon fa-tablet"></span>
  <h2><%=@challenge.name%></h2>
</header>
<div class="container">
  <div class="main">
    <div class="gamma-container gamma-loading" id="gamma-container">

      <ul class="gamma-gallery">
        <% @images.each do |image| %>
        <li>
          <div data-alt="img<%=image.id%>" data-description="<h3><%=image.title%></h3>" data-max-width="1800" data-max-height="1350">
            <div data-src="<%= image.url %>#<%=image.id%>" data-min-width="140"></div>
          </div>
        </li>
        <% end %>
      </ul>

      <div class="gamma-overlay"></div>

      <div id="loadmore" class="loadmore">Example for loading more items...</div>
    </div>

  </div><!--/main-->
</div>
<div id="upvote" style="display: none"><span class="icon fa-arrow-up"></span></div>