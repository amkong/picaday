<body>


 <!-- Header -->
<header id="header" class="alt">
  <nav id="nav-left" class="float-left clearfix">
    <ul>
      <li class="current"><a href="/">Picaday</a></li>
      <%if @current_user.nil? %>
        <li><a href="/auth/sign_up" class="button special">Sign up</a></li>
      <% end %>
    </ul>
  </nav>
  <nav id="nav" class="float-right clearfix">
    <ul>
      <li class="submenu">
        <a href>Actions</a>
        <ul>
            <li><a href="/">Home</a></li>
            <li><a href="/challenges/today">Today</a></li>
            <li><a href="/images">Images</a></li>
            <li><a href="/challenges">Challenges</a></li>
            <li class="submenu">
              <a href="">Submenu</a>
              <ul>
                <li><a href="#">Dolore Sed</a></li>
                <li><a href="#">Consequat</a></li>
                <li><a href="#">Lorem Magna</a></li>
                <li><a href="#">Sed Magna</a></li>
                <li><a href="#">Ipsum Nisl</a></li>
              </ul>
            </li>
        </ul>
      </li>
      <% if @current_user %>
        <li><a href="/logout" class="button special">Sign out</a></li>
      <% else %>
        <li><a href="/auth/sign_in" class="button special">Sign in</a></li>
      <% end %>
    </ul>
  </nav>
</header>


  <%= yield %>
  
  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
  <script src="/javascript/skel.min.js"></script> 
  <script src="/javascript/skel-layers.js"></script> 
  <script src="/javascript/dropotron.js"></script> 
  <script src="/javascript/init.js"></script> 
  <script src="/javascript/application.js"></script> 
</body>



</html>
